{
  "rewrites": [
    {
      "source": "/test",
      "destination": "/Advanced-3D-Testing-Suite.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.glb$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    },
    {
      "source": "/(.*)\\.gltf$", 
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ]
}